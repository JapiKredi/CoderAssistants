{
  "endpoint" : "https://api.groq.com/openai/v1/chat/completions",

  "headers": {
    "Content-Type": "application/json",
    "Authorization": "Bearer <<api_key>>"
  },

  "payload": {
    "model": "<<model>>",
    "messages": [{"role": "user", "content": "<<input_text>>"}],
    "temperature": 0.0,
    "max_tokens": 1024,
    "top_p": 1,
  }
}
